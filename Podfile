project 'howdy-chat-app.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'howdy-chat-app' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for howdy-chat-app
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  
  pod 'GoogleSignIn'
  pod 'TwitterKit'

end
